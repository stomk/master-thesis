journal = {Molecular cytogenetics},
keywords = {Centromere,High resolution cytogenetics,Horse genome,Next generation sequencing,Satellite DNA,[master-thesis],centromere,high resolution cytogenetics,horse,horse genome,next generation sequencing,satellite dna},
mendeley-tags = {[master-thesis],centromere,horse},
