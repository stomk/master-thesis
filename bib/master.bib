@article{Wolfgruber2016,
abstract = {The ancestral centromeres of maize contain long stretches of the tandemly arranged CentC repeat. The abundance of tandem DNA repeats and centromeric retrotransposons (CR) has presented a significant challenge to completely assembling centromeres using traditional sequencing methods. Here, we report a nearly complete assembly of the 1.85 Mb maize centromere 10 from inbred B73 using PacBio technology and BACs fromthe reference genome project. The error rates estimated fromoverlapping BAC sequences are 7 × 10−6 and 5 × 10−5 for mismatches and indels, respectively. The number of gaps in the region covered by the reassembly was reduced from 140 in the reference genome to three. Three expressed genes are located between 92 and 477 kb from the inferred ancestral CentC cluster, which lies within the region of highest centromeric repeat density. The improved assembly increased the count of full-length CR from 5 to 55 and revealed a 22.7 kb segmental duplication that occurred approximately 121,000 years ago. Our analysis provides evidence of frequent recombination events in the form of partial retrotransposons, deletions within retrotransposons, chimeric retrotransposons, segmental duplications including higher order CentC repeats, a deleted CentC monomer, centromere-proximal inversions, and insertion of mitochondrial sequences. Double-strand DNA break (DSB) repair is the most plausible mechanism for these events and may be the major driver of centromere repeat evolution and diversity. In many cases examined here, DSB repair appears to be mediated by microhomology, suggesting that tandem repeats may have evolved to efficiently repair frequent DSBs in centromeres.},
annote = {Sequenced 11 BAC clones spanning centromeric region of chromosome 10 in maize by PacBio (early chemistry).},
author = {Wolfgruber, Thomas K. and Nakashima, Megan M. and Schneider, Kevin L. and Sharma, Anupma and Xie, Zidian and Albert, Patrice S. and Xu, Ronghui and Bilinski, Paul and Dawe, R. Kelly and Ross-Ibarra, Jeffrey and Birchler, James A. and Presting, Gernot G.},
doi = {10.3389/fpls.2016.00308},
file = {:Users/shingo/Dropbox/Papers/Wolfgruber et al.{\_}2016{\_}High Quality Maize Centromere 10 Sequence Reveals Evidence of Frequent Recombination Events.pdf:pdf},
issn = {1664-462X},
journal = {Frontiers in Plant Science},
keywords = {DNA damage repair,DNA loss at centromeres,PacBio,[master-thesis],centromere,centromere evolution,dna damage repair,dna loss at centromeres,hemicentric inversion,illegitimate,illegitimate recombination},
mendeley-tags = {PacBio,[master-thesis],centromere},
number = {308},
pages = {1--14},
pmid = {27047500},
title = {{High Quality Maize Centromere 10 Sequence Reveals Evidence of Frequent Recombination Events}},
url = {http://journal.frontiersin.org/article/10.3389/fpls.2016.00308},
volume = {7},
year = {2016}
}
